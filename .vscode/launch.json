// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Android",
        "type": "reactnative",
        "request": "launch",
        "platform": "android",
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "outDir": "${workspaceFolder}/.vscode/.react"
      },
      {
        "name": "Debug iOS",
        "type": "reactnative",
        "request": "launch",
        "platform": "ios",
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "outDir": "${workspaceFolder}/.vscode/.react"
      }
    ]
  }
  